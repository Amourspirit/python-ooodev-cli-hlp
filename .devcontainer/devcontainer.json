    // Cache extensions in a volume to speed up dev container rebuilds.
    "mounts": [
		"source=vscode-extensions,target=/root/.vscode-server/extensions,type=volume"
	],
    // ðŸ‘‡ Configure tool-specific properties.
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "njpwerner.autodocstring"
